# Supabase Configuration
# Copy this file to .env.local and fill in your values

# Supabase Project URL
NEXT_PUBLIC_SUPABASE_URL=https://[project-ref].supabase.co

# Supabase Publishable Key (safe for client-side)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_...

# Supabase Secret Key (server-side only - NEVER expose in browser)
SUPABASE_SECRET_KEY=sb_secret_...

# GUS REGON API (BIR1) - Polish company registry lookup
# Test key: abcde12345abcde12345 (works with test environment)
GUS_API_KEY=abcde12345abcde12345
GUS_ENVIRONMENT=test
